<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="QMud.Core" assembly="QMud">
<class name="MonsterTemplate" table="monsters" lazy="false">
	<id name="Id" generator="assigned" />
	<property name="Class" />
	<property name="DbFlags" column="Flags" />
	<property name="Name" />
	<property name="Sex" />
	<property name="DbKeywords" column="Keywords" />
	<property name="ShortDescription" column="ShortDesc" />
	<property name="LongDescription" column="LongDesc" />
</class>
</hibernate-mapping>
