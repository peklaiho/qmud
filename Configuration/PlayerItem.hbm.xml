<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="QMud.Core" assembly="QMud">
<class name="PlayerItem" table="player_items" lazy="false">
	<composite-id name="Id" class="CompositeKey">
		<key-property name="Value1" column="PlayerId" />
		<key-property name="Value2" column="Number" />
	</composite-id>
	<property name="ItemId" />
	<property name="Parent" />
	<property name="Location" />
</class>
</hibernate-mapping>
