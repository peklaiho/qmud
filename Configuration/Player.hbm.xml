<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="QMud.Core" assembly="QMud">
<class name="Player" table="players" lazy="false">
	<id name="Id" generator="identity" />
	<property name="Name" />
	<property name="Sex" />
	<property name="Level" />
	<property name="Password" />
	<property name="DbPrefs" column="Preferences" />
	<property name="DbColorPrefs" column="ColorPrefs" />
	<property name="LineLength" />
	<property name="CreatedAt" update="false" />
</class>
</hibernate-mapping>
