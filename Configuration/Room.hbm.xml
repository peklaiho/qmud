<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="QMud.Core" assembly="QMud">
<class name="QRoom" table="rooms" lazy="false">
	<id name="Id" generator="assigned" />
	<property name="ZoneId" />
	<property name="DbCoords" column="Coords" />
	<property name="Terrain" />
	<property name="Name" />
	<property name="DescKey" />
	<property name="DescBegin" />
	<property name="DescEnd" />
</class>
</hibernate-mapping>
