<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="QMud.Core" assembly="QMud">
<class name="QMonsterItem" table="monster_items" lazy="false">
	<composite-id name="Id" class="QCompositeKey">
		<key-property name="Value1" column="MonsterId" />
		<key-property name="Value2" column="Number" />
	</composite-id>
	<property name="Location" />
	<property name="ChoicesStr" column="Choices" />
	<property name="Maximum" />
</class>
</hibernate-mapping>
